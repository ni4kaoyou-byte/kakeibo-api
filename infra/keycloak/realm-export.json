{
  "realm": "kakeibo-dev",
  "enabled": true,
  "displayName": "Kakeibo Dev Realm",
  "registrationAllowed": false,
  "resetPasswordAllowed": true,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "roles": {
    "realm": [
      {
        "name": "USER"
      },
      {
        "name": "ADMIN"
      }
    ]
  },
  "clients": [
    {
      "clientId": "kakeibo-api-dev-client",
      "name": "kakeibo-api-dev-client",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "implicitFlowEnabled": false,
      "serviceAccountsEnabled": false,
      "redirectUris": [
        "http://localhost:*/*"
      ],
      "webOrigins": [
        "*"
      ],
      "defaultClientScopes": [
        "web-origins",
        "profile",
        "roles",
        "email"
      ]
    }
  ],
  "users": [
    {
      "username": "dev-user",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Dev",
      "lastName": "User",
      "email": "dev-user@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "dev-pass",
          "temporary": false
        }
      ],
      "realmRoles": [
        "USER"
      ]
    },
    {
      "username": "dev-admin",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Dev",
      "lastName": "Admin",
      "email": "dev-admin@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "dev-admin-pass",
          "temporary": false
        }
      ],
      "realmRoles": [
        "USER",
        "ADMIN"
      ]
    }
  ]
}
